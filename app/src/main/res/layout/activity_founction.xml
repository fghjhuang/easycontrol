<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.dave.smartbushome.FounctionActivity"
    android:background="#000000"
    android:id="@+id/funacbg">
    <android.support.v7.widget.Toolbar
        android:id="@+id/founction_toolbar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="#00067c"
        style="@style/MenuStyle"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Dark"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/founctionlayout"
        android:layout_below="@+id/founction_toolbar"></FrameLayout>

</RelativeLayout>
