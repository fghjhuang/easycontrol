<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:id="@+id/on"
        android:background="@drawable/ic_ac_power_on_in_room"
        android:visibility="visible"
        android:text="ON"
        android:textSize="30dp"
        android:layout_below="@+id/hvacremark"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:layout_toLeftOf="@+id/space4"
        android:layout_toStartOf="@+id/space4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/off"
        android:background="@drawable/ic_ac_power_off_in_room"
        android:visibility="visible"
        android:textSize="30dp"
        android:text="OFF"
        android:layout_alignTop="@+id/on"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:layout_alignBottom="@+id/on"
        android:layout_toRightOf="@+id/space4"
        android:layout_toEndOf="@+id/space4"
        android:textStyle="bold" />

    <Button
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:id="@+id/down"
        android:background="@drawable/ic_ac_set_temp_down"
        android:visibility="visible"
        android:text="—"
        android:textSize="50dp"
        android:layout_below="@+id/on"
        android:layout_alignLeft="@+id/on"
        android:layout_alignStart="@+id/on"
        android:layout_marginTop="20dp"
        android:textAlignment="center"
        android:gravity="center" />

    <Button
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:id="@+id/up"
        android:background="@drawable/ic_ac_set_temp_up"
        android:visibility="visible"
        android:text="+"
        android:textSize="50dp"
        android:layout_below="@+id/off"
        android:layout_alignRight="@+id/off"
        android:layout_alignEnd="@+id/off"
        android:layout_marginTop="20dp"
        android:textAlignment="center"
        android:gravity="center" />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#bfbfbf"
        android:layout_below="@+id/down"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:id="@+id/textView67" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/autofan"
        android:background="@drawable/ic_ac_fan_speed_auto_in_room"
        android:visibility="visible"
        android:text="AUTO"
        android:layout_below="@+id/mediumfan"
        android:layout_alignLeft="@+id/mediumfan"
        android:layout_alignStart="@+id/mediumfan"
        android:layout_marginTop="10dp"
        android:textSize="16dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/highfan"
        android:background="@drawable/ic_ac_fan_speed_high_in_room"
        android:visibility="visible"
        android:text="High"
        android:layout_below="@+id/down"
        android:layout_alignLeft="@+id/down"
        android:layout_alignStart="@+id/down"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="20dp"
        android:textSize="16dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/mediumfan"
        android:background="@drawable/ic_ac_fan_speed_medium_in_room"
        android:visibility="visible"
        android:text="Medium"
        android:layout_below="@+id/up"
        android:layout_alignRight="@+id/up"
        android:layout_alignEnd="@+id/up"
        android:layout_marginTop="40dp"
        android:layout_marginRight="20dp"
        android:textSize="16dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/lowfan"
        android:background="@drawable/ic_ac_fan_speed_low_in_room"
        android:visibility="visible"
        android:text="Low"
        android:layout_alignLeft="@+id/highfan"
        android:layout_alignStart="@+id/highfan"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/highfan"
        android:textSize="16dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/automode"
        android:background="@drawable/ic_ac_mode_auto_in_room"
        android:visibility="visible"
        android:text="AUTO"
        android:layout_below="@+id/heat"
        android:layout_alignLeft="@+id/heat"
        android:layout_alignStart="@+id/heat"
        android:layout_marginTop="10dp"
        android:textSize="16dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/cool"
        android:background="@drawable/ic_ac_mode_cool_in_room"
        android:visibility="visible"
        android:text="Cool"
        android:layout_below="@+id/lowfan"
        android:layout_alignLeft="@+id/lowfan"
        android:layout_alignStart="@+id/lowfan"
        android:layout_marginTop="40dp"
        android:textSize="16dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/heat"
        android:background="@drawable/ic_ac_mode_heat_in_room"
        android:visibility="visible"
        android:text="Heat"
        android:layout_above="@+id/fan"
        android:layout_alignLeft="@+id/autofan"
        android:layout_alignStart="@+id/autofan"
        android:textSize="16dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/fan"
        android:background="@drawable/ic_ac_mode_fan_in_room"
        android:visibility="visible"
        android:text="fan"
        android:layout_below="@+id/cool"
        android:layout_alignLeft="@+id/cool"
        android:layout_alignStart="@+id/cool"
        android:layout_marginTop="10dp"
        android:textSize="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="0"
        android:id="@+id/temp"
        android:gravity="center"
        android:textSize="50dp"
        android:visibility="visible"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:layout_above="@+id/highfan"
        android:layout_toRightOf="@+id/down"
        android:layout_toLeftOf="@+id/up" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="MEDIUM"
        android:id="@+id/fanstate"
        android:textAlignment="center"
        android:gravity="center"
        android:textSize="16dp"
        android:visibility="visible"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:layout_below="@+id/mediumfan"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="AUTO"
        android:id="@+id/mode"
        android:textAlignment="center"
        android:gravity="center"
        android:textSize="16dp"
        android:visibility="visible"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:layout_below="@+id/heat"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="℃"
        android:id="@+id/uint"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:paddingLeft="15dp"
        android:layout_alignTop="@+id/up"
        android:layout_toRightOf="@+id/mode"
        android:layout_toEndOf="@+id/mode" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="AC:  "
        android:id="@+id/hvacremark"
        android:textIsSelectable="false"
        android:textSize="30dp"
        android:textColor="#ffffff"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:gravity="center" />

    <Space
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/space4" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#bfbfbf"
        android:id="@+id/textView27"
        android:layout_below="@+id/lowfan"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="FAN"
        android:id="@+id/textView66"
        android:gravity="center"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:textSize="30dp"
        android:layout_alignTop="@+id/textView67"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="MODE"
        android:id="@+id/textView68"
        android:gravity="center"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:textSize="30dp"
        android:layout_alignTop="@+id/textView27"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
</merge>