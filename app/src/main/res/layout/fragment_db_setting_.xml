<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mainsetting.DB_settingFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Database Settings:"
        android:id="@+id/textView81"
        android:textColor="#5297ff"
        android:gravity="center"
        android:paddingLeft="5dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="export"
        android:id="@+id/ms_export"
        android:textColor="#5297ff"
        android:layout_below="@+id/textView81"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/media_back"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/database_export"
        android:id="@+id/textView48"
        android:textColor="@color/warmtext2"
        android:paddingLeft="3dp"
        android:layout_below="@+id/ms_export"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="import"
        android:id="@+id/ms_import"
        android:textColor="#5297ff"
        android:layout_below="@+id/textView48"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/media_back"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/database_import"
        android:id="@+id/textView49"
        android:textColor="@color/warmtext2"
        android:paddingLeft="3dp"
        android:layout_below="@+id/ms_import"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="delete  database"
        android:id="@+id/ms_delete"
        android:textColor="#5297ff"
        android:layout_below="@+id/textView49"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/media_back"
        android:layout_marginLeft="2dp"
        android:layout_toLeftOf="@+id/space17"
        android:layout_toStartOf="@+id/space17"
        android:layout_marginRight="5dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share  Database"
        android:id="@+id/ms_share"
        android:textColor="#5297ff"
        android:layout_below="@+id/textView49"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/media_back"
        android:layout_marginRight="2dp"
        android:layout_toEndOf="@+id/space17"
        android:layout_toRightOf="@+id/space17"
        android:layout_marginLeft="5dp" />

    <Space
        android:layout_width="20px"
        android:layout_height="20px"
        android:layout_centerInParent="true"
        android:id="@+id/space17" />
</RelativeLayout>
